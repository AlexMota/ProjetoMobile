<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projetomobile"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.projetomobile.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.projetomobile.MainActivity2"
            android:label="@string/title_activity_main_activity2" >
        </activity>
        <activity
            android:name="com.example.projetomobile.LoginActivity"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name="com.example.projetomobile.MensagemActivity"
            android:label="@string/title_activity_mensagem" >
        </activity>
        <activity
            android:name="com.example.projetomobile.PrincipalActivity"
            android:label="@string/title_activity_principal" >
        </activity>
        <activity
            android:name="com.example.projetomobile.MensagemSelecionadaActivity"
            android:label="@string/title_activity_mensagem_selecionada" >
        </activity>
    </application>

</manifest>
